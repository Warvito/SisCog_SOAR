sp {projSisCog*propose*dispara
   (state <s> ^name projSisCog)
-->
   (<s> ^operator <op> =)
   (<op> ^name dispara)
}

sp {apply*dispara
   (state <s> ^operator <op> 
              ^io <io>)
   (<op> ^name dispara)
   (<io> ^output-link <out>)
   (<out> ^gatilho <g>)
   (<g> ^acionado <st>)
-->
   (<g> ^acionado true)
   (<g> ^acionado <st> -)
}