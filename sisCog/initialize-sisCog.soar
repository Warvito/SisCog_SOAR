sp {propose*initialize-sisCog
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-sisCog)
}

sp {apply*initialize-sisCog
   (state <s> ^operator <op>
              ^io <io>)
   (<op> ^name initialize-sisCog)
   (<io> ^output-link <out>)
-->
   (<s> ^name projSisCog
        ^clock <c>
        ^tiro <t>
        ^status not_ready)        
   (<out> ^fire true)
   (<c> ^estado tic
        ^time 1)
}
