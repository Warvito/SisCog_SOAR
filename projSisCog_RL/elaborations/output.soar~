sp {apply*operator*create-action-command
   (state <s> ^operator <o>
              ^io.output-link <ol>)
   (<o> ^name <n>)
   -(<o> ^name initialize-projSisCog)
-->
   (<ol> ^type.action <n>)
}
#sp {elaborate*remove-action-completed
#   (state <s> ^name projSisCog
#              ^io.output-link <ol>)
#   (<ol> ^status <st>)
#   (<ol> ^action <n>)
#-->
#   (<ol> ^status <st> -)
#   (<ol> ^action <n> -)
#}
sp {apply*operator*remove-command
   (state <s> ^operator <o>
          ^io.output-link <ol>)
   (<ol> ^type <t>)
   (<t>  ^status complete)
-->
   (<ol> ^type <value> -
         ^status <st> - )
}